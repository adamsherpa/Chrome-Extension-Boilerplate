{
    "manifest_version": 3,
    "name": "Boilerplate",
    "version": "0.1.0",
    "description": "Boilerplate extension.",
    "background": {
        "service_worker": "serviceWorker.js"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"]
        }
    ],
    "action": {
        "default_popup": "index.js"
    },
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    }
}